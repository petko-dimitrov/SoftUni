{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cars</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Make</th>
                <th>Model</th>
                <th>Travelled Distance</th>
            </tr>
        </thead>
        {% for car in cars %}
            <tr>
                <td>{{ car.make }}</td>
                <td>{{ car.model }}</td>
                <td>{{ car.travelledDistance }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}